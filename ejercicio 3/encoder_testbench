S_0x5649f3d18cf0 .scope module, "encoder" "encoder_x4" 2 15, 3 7 0, S_0x5649f3d18b70;
 .timescale 0 0;
    .port_info 0 /OUTPUT 2 "z"
    .port_info 1 /OUTPUT 1 "y"
    .port_info 2 /INPUT 4 "x"
L_0x5649f3d39e90 .functor NOT 1, L_0x5649f3d39f60, C4<0>, C4<0>, C4<0>;
L_0x5649f3d3a050 .functor AND 1, L_0x5649f3d39e90, L_0x5649f3d3a110, C4<1>, C4<1>;
