S_0x5649f3d18b70 .scope module, "ecoder_tb" "ecoder_tb" 2 10;
 .timescale 0 0;
v0x5649f3d39c00_0 .var "x", 3 0;
v0x5649f3d39cc0_0 .net "y", 0 0, L_0x5649f3d3a810;  1 drivers
